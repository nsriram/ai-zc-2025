{
  "name": "coding-interview-platform",
  "version": "1.0.0",
  "description": "Real-time collaborative coding interview platform",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend/integration-tests && npm test",
    "test:frontend": "cd frontend/integration-tests && npm test",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install && cd ../backend/integration-tests && npm install && cd ../../frontend/integration-tests && npm install"
  },
  "keywords": [
    "interview",
    "coding",
    "collaboration",
    "real-time"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}