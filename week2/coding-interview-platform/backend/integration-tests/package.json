  "keywords": [
    "integration-tests",
    "jest",
    "supertest"
  ],
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "socket.io-client": "^4.7.2",
    "supertest": "^6.3.3"
  }
}